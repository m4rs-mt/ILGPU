      <dependency id="System.Reflection.Emit.ILGeneration" version="4.3.0" />
      <dependency id="System.Reflection.Metadata" version="1.6.0" />
      <dependency id="System.Runtime.CompilerServices.Unsafe" version="4.5.2" />
    </dependencies>
  </metadata>
