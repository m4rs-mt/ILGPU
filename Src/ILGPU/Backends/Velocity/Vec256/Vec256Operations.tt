<# foreach (var (_, prefix, typeName, _) in ImplementationTypes64) { #>
        [MethodImpl(MethodImplOptions.<#= inliningAttribute #>)]
        internal static <#= warpType64 #> FromScalar<#= prefix #>64(<#= typeName #> scalar)
        {
